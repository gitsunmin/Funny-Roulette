{
    "manifest_version": 2,
    "name": "funny roulette",
    "version": "1.0.0",
    "permissions": ["storage", "experimental", "tts"],
    "background": {
      "scripts": ["build/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon16.png",
      "128": "images/icon16.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["build/injection.js"]
      }
    ]
  }