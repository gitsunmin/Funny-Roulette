{
    "manifest_version": 2,
    "name": "funny roulette",
    "version": "1.0.0",
    "permissions": ["storage", "tts"],
    "background": {
      "scripts": ["build/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }