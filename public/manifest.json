{
    "manifest_version": 2,
    "name": "funny roulette",
    "version": "0.0.1",
    "permissions": ["storage", "experimental"],
    "background": {
      "scripts": ["build/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["build/injection.js"]
      }
    ]
  }